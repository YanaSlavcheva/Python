def nth_fibonacci(n):
    "fibonacci_docstring"

    first_member = 1
    second_member = 1
    third_member = 2
    current_member_number = 3
    result = 1

    if n > 2:
        for _ in range(3, n + 1):
            third_member = first_member + second_member
            current_last_member = third_member       
            first_member = second_member
            second_member = current_last_member
        result = third_member
    return result

# 1 1 2 3 5 8 13 21

# third_number = 2 = 3 = 5
# current_last_number = 2 = 3 = 5
# first_number = 1 = 2 = 3
# second_number = 2 = 3 = 5



